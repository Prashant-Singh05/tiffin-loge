{
  "name": "dabba-hub-frontend",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --lan",
    "start:lan": "expo start --lan --host lan",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clean:deps": "powershell -NoProfile -Command \"if (Test-Path node_modules) { Remove-Item -Recurse -Force node_modules }; if (Test-Path package-lock.json) { Remove-Item -Force package-lock.json }; npm cache clean --force\"",
    "verify:worklets": "node -p \"require('react-native-worklets-core/package.json').version\"",
    "reinstall:sdk54": "npm install && npx expo install react-native-reanimated react-native-gesture-handler && npm install react-native-worklets-core@0.5.0 --save-exact"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.10.1",
    "axios": "^1.6.0",
    "expo": "~54.0.0",
    "expo-auth-session": "~7.0.8",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-location": "~19.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.9",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "babel-preset-expo": "^54.0.7"
  },
  "overrides": {
    "react-native-worklets": "0.5.1"
  },
  "private": true
}
